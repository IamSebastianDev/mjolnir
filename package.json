{
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "bin": {
        "mjolnir": "dist/index.cjs"
    },
    "exports": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs",
        "types": "./dist/types/index.d.ts"
    },
    "types": "dist/types/index.d.ts",
    "license": "MIT",
    "devDependencies": {
        "@commitlint/cli": "^18.4.3",
        "@commitlint/config-conventional": "^18.4.3",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-json": "^6.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@types/node": "^20.2.3",
        "alex": "^11.0.0",
        "ava": "^6.0.1",
        "c8": "^9.1.0",
        "esbuild": "^0.19.8",
        "husky": "^8.0.2",
        "prettier": "^3.1.0",
        "pretty-quick": "^3.1.3",
        "rollup": "^3.23.0",
        "rollup-plugin-add-shebang": "^0.3.1",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-dts": "^6.1.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "semantic-release": "^22.0.10",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2"
    },
    "scripts": {
        "preci": "rm -rf node_modules",
        "ci": "yarn install --frozen-lockfile",
        "reset": "node ./reset.js",
        "setup": "npx husky install",
        "build": " rollup -c .config/rollup.config.js",
        "dev": "yarn build --watch",
        "lint": "alex .",
        "semantic-release": "semantic-release --branches release",
        "test": "ava --verbose",
        "test:coverage": "c8 yarn run test",
        "bin": "node dist/index.cjs"
    },
    "ava": {
        "extensions": {
            "ts": "module"
        },
        "nodeArguments": [
            "--no-warnings",
            "--loader=ts-node/esm",
            "--experimental-specifier-resolution=node"
        ]
    },
    "name": "@iasd/mjolnir",
    "version": "1.1.2",
    "description": "Compiler / Interpreter / Parser for ValhallaScript",
    "author": "Sebastian Heinz <sebsatian@iamsebastian.dev>",
    "publishConfig": {
        "access": "public"
    },
    "private": false,
    "dependencies": {
        "chalk": "^4.1.0"
    }
}
